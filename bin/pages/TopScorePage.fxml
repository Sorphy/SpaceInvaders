<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" spacing="20.0" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="scoreTable" prefHeight="200.0" prefWidth="200.0">
      </TableView>
      <Button fx:id="backButton" alignment="TOP_CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#back" prefHeight="96.0" prefWidth="200.0" text="Back">
         <font>
            <Font size="45.0" />
         </font>
      </Button>
   </children>
</VBox>
